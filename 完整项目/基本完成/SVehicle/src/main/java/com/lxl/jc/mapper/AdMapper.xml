<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.lxl.jc.mapper.AdMapper">
	
	<resultMap type="a" id="adMap">
		<result property="apname" column="apname" />
		<result property="acname" column="acname"/>
		<result property="astate" column="astate"/>
		<result property="atime" column="atime"/>
